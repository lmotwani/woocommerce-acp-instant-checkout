{
    "name": "woocommerce-acp-instant-checkout/woocommerce-acp",
    "description": "WooCommerce integration for OpenAI's Agentic Commerce Protocol (ACP) enabling 'Buy it in ChatGPT' functionality",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "WooCommerce ACP Team",
            "email": "support@example.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "stripe/stripe-php": "^10.0",
        "justinrainbow/json-schema": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "WooCommerceACP\\": "includes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "scripts": {
        "build": [
            "@composer install --no-dev --optimize-autoloader"
        ]
    }
}
